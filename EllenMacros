    Selection.Find.Replacement.ClearFormatting
    
    If x > 0 Then
    MsgBox ("Check for templated language!")
    
    Else
    MsgBox ("All good!")
    
    End If

End Sub



Sub MegaMacro()
'
' MegaMacro Macro
'

'' AndSymbol Macro
Selection.Find.ClearFormatting
Selection.Find.Replacement.ClearFormatting
With Selection.Find
        .Text = " & "
        .Replacement.Text = " and "
        .Forward = True
        .Wrap = wdFindContinue
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "Russ College of Engineering and"
        .Replacement.Text = "Russ College of Engineering &"
        .Forward = True
        .Wrap = wdFindContinue
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "Larsen and Toubro"
        .Replacement.Text = "Larsen & Toubro"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

