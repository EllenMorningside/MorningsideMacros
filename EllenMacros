Sub verbTense4()

        ActiveDocument.TrackRevisions = Not ActiveDocument.TrackRevisions
    
    Dim verbTense(200, 3) As String
    Dim x As Integer

verbTense(1, 1) = "automated": verbTense(1, 2) = "automate": verbTense(1, 3) = "N"
verbTense(2, 1) = "built": verbTense(2, 2) = "build": verbTense(2, 3) = "N"
verbTense(3, 1) = "worked": verbTense(3, 2) = "work": verbTense(3, 3) = "N"
verbTense(4, 1) = "" : verbTense(4, 2) = "" : verbTense(4, 3) = "N"

For x = 1 To 200

    Selection.Find.ClearFormatting
    Selection.Find.Replacement.ClearFormatting
    With Selection.Find
        .Text = verbTense(x, 1)
        .Replacement.Text = verbTense(x, 2)
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = False
        .MatchWholeWord = True
        If verbTense(x, 3) = "Y" Then
            .MatchCase = True
        Else
            .MatchCase = False
        End If
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    Next
    
End Sub
    
Sub MegaMacro()
'
' MegaMacro Macro
'

'' AndSymbol Macro
Selection.Find.ClearFormatting
Selection.Find.Replacement.ClearFormatting
With Selection.Find
        .Text = " & "
        .Replacement.Text = " and "
        .Forward = True
        .Wrap = wdFindContinue
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "Russ College of Engineering and"
        .Replacement.Text = "Russ College of Engineering &"
        .Forward = True
        .Wrap = wdFindContinue
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "Larsen and Toubro"
        .Replacement.Text = "Larsen & Toubro"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

With Selection.Find
        .Text = "Larsen and Toubro Infotech Limited"
        .Replacement.Text = "Larsen & Toubro Infotech Limited"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "LARSEN and TOUBRO"
        .Replacement.Text = "LARSEN & TOUBRO"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = True
        .MatchWholeWord = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
    With Selection.Find
        .Text = "Deloitte and Touche"
        .Replacement.Text = "Deloitte & Touche"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
    With Selection.Find
        .Text = "DELOITTE and TOUCHE"
        .Replacement.Text = "DELOITTE & TOUCHE"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
