Sub MegaMacro()
'
' MegaMacro Macro
'

'' AndSymbol Macro
Selection.Find.ClearFormatting
Selection.Find.Replacement.ClearFormatting
With Selection.Find
        .Text = " & "
        .Replacement.Text = " and "
        .Forward = True
        .Wrap = wdFindContinue
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "Russ College of Engineering and"
        .Replacement.Text = "Russ College of Engineering &"
        .Forward = True
        .Wrap = wdFindContinue
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "Larsen and Toubro"
        .Replacement.Text = "Larsen & Toubro"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll

With Selection.Find
        .Text = "Larsen and Toubro Infotech Limited"
        .Replacement.Text = "Larsen & Toubro Infotech Limited"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
With Selection.Find
        .Text = "LARSEN and TOUBRO"
        .Replacement.Text = "LARSEN & TOUBRO"
        .Forward = True
        .Wrap = wdFindContinue
        .Format = False
        .MatchCase = True
        .MatchWholeWord = False
        .MatchWildcards = False
        .MatchSoundsLike = False
        .MatchAllWordForms = False
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
    With Selection.Find
        .Text = "Deloitte and Touche"
        .Replacement.Text = "Deloitte & Touche"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
    With Selection.Find
        .Text = "DELOITTE and TOUCHE"
        .Replacement.Text = "DELOITTE & TOUCHE"
        .Forward = True
        .Wrap = wdFindContinue
        .MatchCase = True
    End With
    Selection.Find.Execute Replace:=wdReplaceAll
    
